{
  "name": "shadow-emotion-esbuild-typescript-webpack",
  "version": "1.0.0",
  "description": "Webpack esbuild emotion shadow-dom typescript react examples",
  "keywords": [
    "webpack",
    "react",
    "typescript",
    "emotion",
    "sass",
    "hmr",
    "esbuild",
    "shadow-dom",
    "boilerplate"
  ],
  "author": "Adan Perez",
  "license": "MIT",
  "scripts": {
    "build": "yarn run clean-dist && webpack --config=configs/webpack/prod.js",
    "clean-dist": "rimraf dist/*",
    "lint": "eslint './src/**/*.{js,ts,tsx}' --quiet",
    "start": "yarn run start-dev",
    "start-dev": "webpack serve --config=configs/webpack/dev.js",
    "start-prod": "yarn run build && node express.js",
    "test": "jest --coverage --watchAll --config=configs/jest.json"
  },
  "devDependencies": {
    "@emotion/cache": "11.4.0",
    "@emotion/css": "11.1.3",
    "@emotion/react": "11.4.1",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.5",
    "@types/react": "^17.0.29",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "css-loader": "^6.4.0",
    "esbuild-loader": "^2.16.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "express": "^4.17.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "image-webpack-loader": "^8.0.1",
    "jest": "^27.2.5",
    "node-sass": "^6.0.1",
    "prettier": "^2.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hot-loader": "^4.13.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^12.2.0",
    "style-loader": "^3.3.0",
    "typescript": "^4.4.4",
    "webpack": "^5.58.2",
    "webpack-cli": "^4.9.0",
    "webpack-dev-server": "^4.3.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {}
}
